package csd;

class App {
	public static void main(String [] args)
	{	
		SumFactorsTest.run();
	}
}

class SumFactorsTest {
	public static void run()
	{
		java.util.Scanner kb = new java.util.Scanner(System.in);
		
		System.out.print("Bir sayı giriniz:");
		int val = kb.nextInt();
		
		System.out.printf("%d sayısının çarpanlarının toplamı:%d%n", val, NumberUtil.sumFactors(val));
	}
}

class NumberUtil {
	public static int sumFactors(int val)
	{
		if (val <= 0)
			return -1;
		
		int total = 1;
		int a = 2;
		
		while (a < val) {
			if (val % a == 0)
				total += a;
			
			++a;
		}
		
		return total;
	}
}
